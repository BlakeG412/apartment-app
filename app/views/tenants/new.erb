<form action="/tenants" method="post">


<label>Tenant:</label>
<input type="text" name="tenant[name]"/>

<label>Existing Address:</label>
<select name="apartment[apartment_id]">
<% @apartments.each do |apt|%>
  <option value=<%= apt.id %>> <%= apt.address %> </option>
<% end %>
</select>


<label>New Address:</label>
<input type="text" name="apartment[address]">

<input type="submit"/>

</form>
